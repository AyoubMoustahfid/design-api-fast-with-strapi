{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "name": "Reservation"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "richtext",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "cin": {
      "type": "string",
      "required": true
    },
    "product": {
      "model": "restaurant"
    }
  }
}
